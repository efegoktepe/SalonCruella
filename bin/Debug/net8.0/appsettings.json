{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SalonCruellaDB;Trusted_Connection=True;",
    "SalonCruellaIdentityDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=SalonCruella;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Identity": {
    "LoginPath": "/Account/Login",
    "LogoutPath": "/Account/Logout",
    "AccessDeniedPath": "/Account/AccessDenied"
  },
  "PasswordPolicy": {
    "RequireDigit": true,
    "RequiredLength": 8,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": true,
    "RequireLowercase": true
  }
}
